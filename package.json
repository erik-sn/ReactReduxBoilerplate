{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate with support for Redux, Hot Reloading, Routing, Isomorphic rendering, ESLint and SASS",
  "main": "index.js",
  "repository": "",
  "scripts": {
    "dev": "node server.js",
    "lint": "eslint ./src ./test",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "coverage": "istanbul cover _mocha --require helper.js -- --compilers js:babel-core/register --recursive test/*",
    "build": "webpack -p --config webpack.production.config.js --progress --profile --colors",
    "start": "nodemon server.production.js --exec babel-node"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "es6-promise": "~4.0.5",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.0",
    "mocha": "^3.1.2",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^1.1.0",
    "react-addons-test-utils": "^15.3.2",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.4"
  }
}
